{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/api/**",
        "function": "getItems"
      }
    ]
  },
  "functions": [
    {
      "codebase": "default",
      "source": "functions",
      "runtime": "python313",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "emulators": {
    "hosting": {
      "host": "127.0.0.1",
      "port": 5000
    },
    "functions": {
      "host": "127.0.0.1",
      "port": 5001
    },
    "ui": {
      "host": "127.0.0.1",
      "port": 4000
    }
  }
}